<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <record forcecreate="True" id="pos_order_out_stock_email" model="ir.cron">
	       <field name="name">Pos order out of stock - Email</field>
	       <field name="model_id" ref="point_of_sale.model_pos_order"/>
